<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>CrystalQuartz.Web</id>
    <version>1.1</version>
    <authors>Eugene Guryanov</authors>
    <owners>Jimmy Stridh</owners>
    <licenseUrl>http://www.apache.org/licenses/LICENSE-2.0</licenseUrl>
    <projectUrl>https://github.com/visit/crystalquartz</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Installs CrystalQuartz panel (pluggable Qurtz.NET viewer) using simple scheduler provider.
      This approach is appropriate for scenarios where the scheduler and
      a web application works in the same AppDomain.
    </description>
    <tags>.NET, ASP.NET, Quartz.NET, Scheduler, Job, Trigger, UI, Admin, Console</tags>
    <dependencies>
        <dependency id="NVelocity" version="1.0.3" />
        <dependency id="Quartz" version="2.0.1" />
    </dependencies>
  </metadata>
  <files>
      <file src="..\..\bin\Release\CrystalQuartz.Web.dll" target="lib\net40\CrystalQuartz.Web.dll" />
      <file src="..\..\bin\Release\CrystalQuartz.Core.dll" target="lib\net40\CrystalQuartz.Core.dll" />
  </files>
</package>